CREATE SEQUENCE SEQ_accounts START WITH 1 INCREMENT BY 1;
CREATE SEQUENCE SEQ_events START WITH 1 INCREMENT BY 1;


drop SEQUENCE SEQ_accounts;
CREATE TABLE accounts(
    Id INTEGER NOT NULL PRIMARY KEY,
    nome VARCHAR (50) NOT NULL,
    email VARCHAR2 (50) NOT NULL UNIQUE ,
    Password VARCHAR2 (16) NOT NULL,
    birthdate varchar2(20) NOT NULL,
    carteira FLOAT,
    funcao varchar(10)
    );
    
insert into accounts VALUES(SEQ_accounts.NEXTVAL,:name, :email, :password, :birthdate, 0,NULL);
Select Id FROM accounts where nome=:name and email = :email and password = :password;


CREATE TABLE events(
    id integer not null,
    token VARCHAR2 (16) NOT NULL PRIMARY KEY,
    titulo varchar2(50),
    descr varchar2(150),
    data_aposta date,
    inicio time(10),
    fim time(10),
    valoraposta number(10),
    aprova varchar2(10)
);
drop TABLE events;
drop table accounts;


Select * FROM events where data<=CONVERT(date,GETUTCDATE())or
(data<=CONVERT(date,GETUTCDATE()) and fim<CONVERT(time,GETUTCDATE()));

